{
  "name": "overtype-mode",
  "description": "Overtype mode of text-editor",
  "repository": "https://github.com/asiloisad/pulsar-overtype-mode",
  "keywords": [
    "overtype",
    "mode"
  ],
  "version": "2.0.1",
  "license": "MIT",
  "main": "./lib/main",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "configSchema": {
    "statusBar": {
      "order": 1,
      "title": "Status bar icon",
      "description": "Show status icon in the status bar.",
      "type": "boolean",
      "default": true
    }
  }
}
